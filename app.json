{
  "expo": {
    "name": "showroom",
    "slug": "showroom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.minhductxqt.showroom",
      "supportsTablet": true
    },
    "android": {
      "package": "com.minhductxqt.showroom",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6c25b9df-1337-4146-9dbc-e6c5bfe455c7"
      }
    },
    "scheme": "showroom"
  }
}
